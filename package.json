{
  "name": "reactjs-typescript-example",
  "version": "0.0.1",
  "description": "reactjs-typescript-example",
  "repository": {
    "type": "git"
  },
  "license": "MIT",
  "private": true,
  "main": "index.js",
  "scripts": {
    "build": "webpack -p --progress --config webpack.prod.js",
    "dev-build": "webpack --progress -d --config webpack.dev.js",
    "start": "webpack-dev-server --hot --inline --config webpack.dev.js",
    "test": "jest"
  },
  "author": "hk0211",
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.15",
    "@types/jest": "^23.3.11",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "@types/react-redux": "^6.0.11",
    "awesome-typescript-loader": "^5.2.1",
    "babel-core": "^6.25.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^1.0.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-redux": "^6.0.0",
    "redux": "^4.0.1",
    "source-map-loader": "^0.2.4",
    "ts-jest": "^23.10.5",
    "typescript": "^3.2.2",
    "url-loader": "^1.1.2",
    "webpack": "^4.0.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.1.5"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/setupTests.js",
    "transform": {
      "^.+\\.js?$": "babel-jest",
      "^.+\\.(ts|tsx)$": "./node_modules/ts-jest/preprocessor.js"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testMatch": [
      "**/tests/*.test.(ts|tsx)"
    ]
  },
  "dependencies": {}
}
